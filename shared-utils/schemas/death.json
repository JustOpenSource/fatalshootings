{
    "id": "/fe/death/v1",
    "type": "object",
    "properties": {

        "cause" : {
            "enum": [
                "Asphyxiation",
                "Assault",
                "Automobile",
                "Drug Related",
                "Gunshot",
                "Medical Emergency",
                "Neglect",
                "Non-Lethal Weapon",
                "Suicide",
                "Unknown",
                "Other"
            ]
        },

        "cause_notes" : {
            "type": "string"
        },

        "responsible_agency" : {
            "type": "string"
        },

        "description" : {
            "type": "string"
        },

        "date" : {
            "type": "string",
            "pattern" : "^([0-9]{1,4})\d\d[/](0[1-9]|1[012])[/](0[1-9]|[12][0-9]|3[01])$" //yyyy/mm/dd
        },

        "disposition" : {
            "type": {
                "enum" : [
                    "Justified",
                    "Homocide",
                    "Other"
                ]
            }
        }
    }
}