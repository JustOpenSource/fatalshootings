<div class="page-content">
    <div class="container">
        <div class="page-header">
            <h1>Install clean records</h1>
        </div>
        <div class="row">
            <div class="col-md-6">
                <h2>{{title}}</h2>
                {{message}}
            </div>
            <div class="col-md-6">
                <h2>Log</h2>
                <ul class="list-group">
                    {{#each actions}}
                        <li class="list-group-item"><strong>{{label}}</strong><span class="pull-right">{{value}}</span></li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>