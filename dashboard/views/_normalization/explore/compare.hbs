<div class="page-content">
    <div class="container">
        <div class="page-header">
            <h1>Results Comparison</h1>
        </div>
        <div class="row">
            {{#if results}}
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>Location</th>
                            <th>Original</th>
                            <th>Formatted</th>
                            <th>Normalized</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each results}}
                            <tr>
                                <td colspan="4"><h2>{{this.orig.id}}</h2></td>
                            </tr>
                            <tr>
                                <td><strong>death.cause</strong></td>
                                <td>{{this.orig.doc.causeofdeath}}</td>
                                <td>{{this.dirty.value.death.cause}}</td>
                                <td>{{this.clean.value.death.cause}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.cause_notes</strong></td>
                                <td></td>
                                <td>{{this.dirty.value.death.cause_notes}}</td>
                                <td>{{this.clean.value.death.cause_notes}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.responsible_agency</strong></td>
                                <td>{{this.orig.doc.agencyresponsiblefordeath}}</td>
                                <td>{{this.dirty.value.death.responsible_agency}}</td>
                                <td>{{this.clean.value.death.responsible_agency}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.description</strong></td>
                                <td>{{this.orig.doc.abriefdescriptionofthecircumstancessurroundingthedeath}}</td>
                                <td>{{this.dirty.value.death.description}}</td>
                                <td>{{this.clean.value.death.description}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.disposition</strong></td>
                                <td>{{this.orig.doc.officialdispositionofdeathjustifiedorother}}</td>
                                <td>{{this.dirty.value.death.disposition}}</td>
                                <td>{{this.clean.value.death.disposition}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.source_url</strong></td>
                                <td>{{this.orig.doc.linktonewsarticleorphotoofofficialdocument}}</td>
                                <td>{{this.dirty.value.death.source_url}}</td>
                                <td>{{this.clean.value.death.source_url}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.event.address</strong></td>
                                <td>{{this.orig.doc.locationofinjuryaddress}}</td>
                                <td>{{this.dirty.value.death.event.address}}</td>
                                <td>{{this.dirty.value.death.event.address}}</td>
                            </tr>
                            <tr>
                                <td><strong>death.event.date</strong></td>
                                <td>{{this.orig.doc.dateofinjuryresultingindeathmonthdayyear}}</td>
                                <td>{{this.dirty.value.death.event.date}}</td>
                                <td>{{this.dirty.value.death.event.date}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.age</strong></td>
                                <td>{{this.orig.doc.subjectsage}}</td>
                                <td>{{this.dirty.value.subject.age}}</td>
                                <td>{{this.dirty.value.subject.age}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.sex</strong></td>
                                <td>{{this.orig.doc.subjectsgender}}</td>
                                <td>{{this.dirty.value.subject.sex}}</td>
                                <td>{{this.dirty.value.subject.sex}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.name</strong></td>
                                <td>{{this.orig.doc.subjectsname}}</td>
                                <td>{{this.dirty.value.subject.name}}</td>
                                <td>{{this.dirty.value.subject.name}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.race</strong></td>
                                <td>{{this.orig.doc.subjectsrace}}</td>
                                <td>{{this.dirty.value.subject.race}}</td>
                                <td>{{this.dirty.value.subject.race}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.image_url</strong></td>
                                <td>{{this.orig.doc.urlofimageofdeceased}}</td>
                                <td>{{this.dirty.value.subject.image_url}}</td>
                                <td>{{this.dirty.value.subject.image_url}}</td>
                            </tr>
                            <tr>
                                <td><strong>subject.mental_illness</strong></td>
                                <td>{{this.orig.doc.symptomsofmentalillness}}</td>
                                <td>{{this.dirty.value.subject.mental_illness}}</td>
                                <td>{{this.dirty.value.subject.mental_illness}}</td>
                            </tr>
                            <tr>
                                <td><strong>location.city</strong></td>
                                <td>{{this.orig.doc.locationofdeathcity}}</td>
                                <td>{{this.dirty.value.location.city}}</td>
                                <td>{{this.dirty.value.location.city}}</td>
                            </tr>
                            <tr>
                                <td><strong>location.county</strong></td>
                                <td>{{this.orig.doc.locationofdeathcounty}}</td>
                                <td>{{this.dirty.value.location.county}}</td>
                                <td>{{this.dirty.value.location.county}}</td>
                            </tr>
                            <tr>
                                <td><strong>location.state</strong></td>
                                <td>{{this.orig.doc.locationofdeathstate}}</td>
                                <td>{{this.dirty.value.location.state}}</td>
                                <td>{{this.dirty.value.location.state}}</td>
                            </tr>
                            <tr>
                                <td><strong>location.zipcode</strong></td>
                                <td>{{this.orig.doc.locationofdeathzipcode}}</td>
                                <td>{{this.dirty.value.location.zipcode}}</td>
                                <td>{{this.dirty.value.location.zipcode}}</td>
                            </tr>
                            <tr>
                                <td><strong>submitted_by</strong></td>
                                <td>{{this.orig.doc.uniqueidentifiersubmittedby}}</td>
                                <td>{{this.dirty.value.submitted_by}}</td>
                                <td>{{this.dirty.value.submitted_by}}</td>
                            </tr>
                            <tr>
                                <td><strong>published</strong></td>
                                <td>[new]</td>
                                <td>{{this.dirty.value.published}}</td>
                                <td>{{this.dirty.value.published}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                    <tfoot>
                        <tr>
                            <th>Location</th>
                            <th>Original</th>
                            <th>Formatted</th>
                            <th>Normalized</th>
                        </tr>
                    </tfoot>
                </table>
            {{else}}
                <p>No results</p>
            {{/if}}
        </div>
    </div>
</div>